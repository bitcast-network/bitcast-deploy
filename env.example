#
# Master env for deploying Bitcast validators
# Copy this file to .env and fill in the values.
#

# Wallet / network
WALLET_NAME=
HOTKEY_NAME=
NETUID=93
SUBTENSOR_NETWORK=finney
SUBTENSOR_CHAIN_ENDPOINT=wss://entrypoint-finney.opentensor.ai:443

# Optional: Full validation API keys (required for Bitcast video validator; optional for Bitcast X in full mode)
RAPID_API_KEY=
CHUTES_API_KEY=
WANDB_API_KEY=

# Optional toggles
DISABLE_AUTO_UPDATE=true

# Optional logging
LOGGING=--logging.info

# Optional ports (only if you need overrides)
#PORT=8092

# Optional: override repo locations if not using defaults
#BITCAST_DIR=/home/ubuntu/bitcast
#BITCAST_X_DIR=/home/ubuntu/bitcast-x
